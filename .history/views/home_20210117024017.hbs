<div class="mainbox">
        {{>main/headerNavbar}}
        <div class="row" style="margin: 5vh 7% 3vh 7%;">
        <div class="col-sm" style="padding: 0">
            <h2 id="gioithieu" style="border-bottom: 1px solid #444444;padding-bottom: 10px">
            Most Popular
        </h2>
        </div>
        </div>
        <div class="carousel-size">
            <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                <ol class="carousel-indicators">
                    {{#each course}}
                    <li data-target="#carouselExampleIndicators" data-slide-to="{{@key}}" {{#if_eq @key 0}}class="active"{{/if_eq}}></li>
                    {{/each}}
                </ol>
                <div class="carousel-inner">
                    {{#each course}}
                    <div class="carousel-item {{#if_eq @key 0}}active{{/if_eq}}">
                        <img class="d-block imgBig" src="{{img}}" alt="First slide">
                        <div class="carousel-caption d-none d-md-block fade">
                            <h5 style="background-color:rgba(255,255,255, 0.7); color:black">{{name}}</h5>
                            <h6 style="background-color:rgba(255,255,255, 0.7)">{{description1}}</h6>
                        </div>
                    </div>
                    {{/each}}
                </div>
                <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev" style="margin-left: -10%; filter: invert(100%);">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next" style="margin-right: -10%;  filter: invert(100%);">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>
        </div>
        <div class="row" style="margin: 5vh 7% 2vh 7%;">
        <div class="col-sm-10" style="padding: 0">
            <h2 id="gioithieu" style="border-bottom: 1px solid #444444;padding-bottom: 10px">
            Top Viewed
        </h2>
        </div>
        <div class="col-sm-2" style="padding: 0">
            <div style="border-bottom: 1px solid #444444;padding-bottom: 10px; padding-left: 20%;">
            <a class="btn btn-secondary" style="margin: 0 5px 0 5px; padding-left: 20px; padding-right: 20px" href="#multi-item-example" data-slide="prev"><i class="fa fa-chevron-left"></i></a>
            <a class="btn btn-secondary" style="margin: 0 5px 0 5px; padding-left: 20px; padding-right: 20px" href="#multi-item-example" data-slide="next"><i class="fa fa-chevron-right"></i></a>
            </div>
        </div>
        </div>
        <div id="multi-item-example" class="card-deck-row carousel slide carousel-multi-item"  data-interval="false">
            <div class="carousel-inner" role="listbox">
            {{#each courses}}
            {{#if_eq (math @key "%" 5) 0}}<div class="carousel-item {{#if_eq @key 0}}active{{/if_eq}}"><div class="row">{{/if_eq}}
            <div class="card">
                <a href="detail/courseid={{idcourses}}" class="stretched-link"><img class="card-img-top" src="{{img}}" alt="{{@key}}"></a>
                <div class="card-body">
                    <h5 class="card-title">{{name}}</h5>
                    <p class="card-text">{{description1}}</p>
                </div>
                <div class="card-footer">
                    <small class="text-muted">Last Update: {{lastupdate}}</small>
                </div>
            </div>
            {{#if_eq (math @key "%" 5) 4}}</div>
            </div>{{/if_eq}}
            {{#if_eq (math @key "+" 1) ../courses.length}}</div>
            </div>{{/if_eq}}
            {{/each}}
            </div>
        </div>

        <div class="row" style="margin: 5vh 7% 0 7%;">
        <div class="col-sm-10" style="padding: 0">
            <h2 id="gioithieu" style="border-bottom: 1px solid #444444;padding-bottom: 10px">
            Top Newest
        </h2>
        </div>
        <div class="col-sm-2" style="padding: 0">
            <div style="border-bottom: 1px solid #444444;padding-bottom: 10px; padding-left: 20%;">
            <a class="btn btn-secondary" style="margin: 0 5px 0 5px; padding-left: 20px; padding-right: 20px" href="#multi-item-example2" data-slide="prev"><i class="fa fa-chevron-left"></i></a>
            <a class="btn btn-secondary" style="margin: 0 5px 0 5px; padding-left: 20px; padding-right: 20px" href="#multi-item-example2" data-slide="next"><i class="fa fa-chevron-right"></i></a>
            </div>
        </div>
        </div>
        <div id="multi-item-example2" class="card-deck-row carousel slide carousel-multi-item"  data-interval="false">
            <div class="carousel-inner" role="listbox">
            {{#each courses}}
            {{#if_eq (math @key "%" 5) 0}}<div class="carousel-item {{#if_eq @key 0}}active{{/if_eq}}"><div class="row">{{/if_eq}}
            <div class="card">
                <a href="detail/courseid={{idcourses}}" class="stretched-link"><img class="card-img-top" src="{{img}}" alt="{{@key}}"></a>
                <div class="card-body">
                    <h5 class="card-title">{{name}}</h5>
                    <p class="card-text">{{description1}}</p>
                </div>
                <div class="card-footer">
                    <small class="text-muted">Last Update: {{lastupdate}}</small>
                </div>
            </div>
            {{#if_eq (math @key "%" 5) 4}}</div>
            </div>{{/if_eq}}
            {{#if_eq (math @key "+" 1) ../courses.length}}</div>
            </div>{{/if_eq}}
            {{/each}}
            </div>
        </div>

        <div class="row" style="margin: 5vh 7% 0 7%;">
        <div class="col" style="padding: 0">
            <h2 id="gioithieu" style="border-bottom: 1px solid #444444;padding-bottom: 10px">
            Top popular Categories 
        </h2>
        </div>
        </div>
    </div>
    {{>main/footer}}
    