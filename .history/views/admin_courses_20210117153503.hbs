
    <!-- ============================================================== -->
    <!-- main wrapper -->
    <!-- ============================================================== -->
    <div class="dashboard-main-wrapper">
    <style>
        td{
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
        }
    </style>
        {{>admin/navbar}}
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- left sidebar -->
        <!-- ============================================================== -->
        {{> admin/sidemenu}}
        <!-- ============================================================== -->
        <!-- end left sidebar -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- wrapper  -->
        <!-- ============================================================== -->
        <div class="dashboard-wrapper">
                <div class="container-fluid dashboard-content ">
                    <!-- ============================================================== -->
                    <!-- pageheader  -->
                    <!-- ============================================================== -->
                    {{>admin/pageheader}}
                    <!-- ============================================================== -->
                    <!-- end pageheader  -->
                    <!-- ============================================================== -->

                <div class="row">
                    <!-- ============================================================== -->
                    <!-- basic table  -->
                    <!-- ============================================================== -->
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                        <div class="card">
                            <h5 class="card-header">Basic Table</h5>
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table class="table table-striped table-bordered first">
                                        <thead>
                                            <tr>
                                                <th>ID</th>
                                                <th>Name</th>
                                                <th>Teacher</th>
                                                <th>Rate</th>
                                                <th>Rate Votes</th>
                                                <th>Price</th>
                                                <th>Last Update</th>
                                                <th>Complete</th>
                                                <th>Subscribes</th>
                                                <th>Option</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#each courses}}
                                            <tr>
                                                <td>{{idcourses}}</td>
                                                <td>{{name}}</td>
                                                <td>{{teacherName}}</td>
                                                <td>{{rate}}</td>
                                                <td>{{ratevotes}}</td>
                                                <td>${{price}}</td>
                                                <td>{{lastupdate}}</td>
                                                <td>{{isCompleted}}</td>
                                                <td>{{subscribes}}</td>
                                                <td style="text-align: center;">
                                                    <ul class="list-inline m-0">
                                                    {{#if_eq isBlocked 1}}
                                                <li class="list-inline-item">
                                                    <button class="btn btn-danger btn-sm rounded-0" type="button" data-toggle="tooltip" data-placement="top" title="Unlock" id="unlock{{idcourses}}" onclick="unlockClick({{idcourses}},'courses/unlockcourse/{{idcourses}}')"><i class="fa fa-lock"></i></button>
                                                </li>
                                                {{else}}
                                                <li class="list-inline-item">
                                                    <button class="btn btn-light border btn-sm rounded-0" type="button" data-toggle="tooltip" data-placement="top" title="Lock" id="lock{{idcourses}}" onclick="lockClick({{idcourses}},'courses/lockcourse/{{idcourses}}')"><i class="fa fa-lock-open"></i></button>
                                                </li>
                                                {{/if_eq}}
                                                </ul>
                                                </td>
                                            {{/each}}
                                            </tr>
                                        </tbody>
                                        <tfoot>
                                            <tr>
                                                <th>ID</th>
                                                <th>Name</th>
                                                <th>Teacher</th>
                                                <th>Rate</th>
                                                <th>Rate Votes</th>
                                                <th>Price</th>
                                                <th>Last Update</th>
                                                <th>Complete</th>
                                                <th>Subscribes</th>
                                                <th>Option</th>
                                            </tr>
                                            
                                        </tfoot>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- ============================================================== -->
                    <!-- end basic table  -->
                    <!-- ============================================================== -->
                </div>
                
            </div>
        {{>admin/footer}}
        </div>
        <!-- ============================================================== -->
        <!-- end wrapper  -->
        <!-- ============================================================== -->
    </div>
    <script src="/js/addbutton.js"></script>
    <!-- ============================================================== -->
    <!-- end main wrapper  -->

